<%- include('../includes/Header') %>

<div class="row">
    <div class="col-md-6 offset-md-3">
        <table class="table table-dark table-striped">
<tr>
    <th>Sr No.</th>
    <th>FullName</th>
    <th>Salary</th>
    <th>City</th>
    <th>Day</th>
    <th>Month</th>
    <th>Year</th>
    <th>Address</th>
    <th>Edit</th>
    <th>Delete</th>
</tr>

<% result.forEach((x,n)=> { %>
    <tr>
        <td><%= n+1 %></td>
        <td><%= x.fullname %></td>
        <td><%= x.salary %></td>
        <td><%= x.city %></td>
        <td><%= x.day %></td>
        <td><%= x.month %></td>
        <td><%= x.year %></td>
        <td><%= x.address %></td>
       <td><a href="#" class="btn btn-primary">Edit</a></td>
       <td><a href="#" class="btn btn-danger">Delete</a></td>
      
    </tr>
    
<% }) %>



</table>

    </div>
</div>

</body>
</html>