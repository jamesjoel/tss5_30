<%- include("../includes/header") %>
<div class="container">
    <div class="row">
        <div class="col-md-12 my-4">
            <% if(result.length==0){ %>
                <div class="alert alert-danger">No record found in employee</div>
           <% }else {%>
           <table class="table table-dark">
            <tr>
                <th>S.no.</th>
                <th>Name</th>
                <th>City</th>
                <th>salary</th>
            </tr>
            <%  result.forEach((x,n)=>{ %>
            <tr> 
             <td><%= n+1 %></td>
             <td><%= x.name  %></td>
             <td><%= x.city  %></td>
             <td><%= x.salary %></td>
              <%  })  %>
            </tr>
        </table>
        <% } %>
                    </div>
    </div>
</div>


</body>
</html>