 <%- include("../includes/header") %>     

 <div class="container my-4">
    <div class="row">
        <div class="col-md-12">
            <table class=" table table-dark">
                <tr>
                    <td>S.no.</td>
                    <td>Name</td>
                    <td>Salary</td>
                    <td>City</td>
                    <th>Detail</th>
                    <th>Edit</th>
                    <th>Delete</th>

                </tr>
                <% result.forEach((x,n)=>{ %>
                    <tr>
                   <td><%= n+1 %> </td>
                   <td><%= x.name %> </td>
                   <td><%= x.salary %> </td>
                    <td><%= x.city %> </td> 
                    <td><a href="/employee/detail/<%= x._id %>" class="btn btn-sm btn-primary">Detail</a></td>
                   <td><a href="/employee/detail/<%= x._id %>" class="btn btn-sm btn-primary">Detail</a></td>
                        <td><a href="#" class="btn btn-sm btn-info">Edit</a></td>
                        <td><a href="/employee/delete/<%= x._id %>" class="btn btn-sm btn-danger">Delete</a></td>

                    </tr>
                 <% }) %> 
                
            </table>
        </div>
    </div>
</div>

</body>
</html>
