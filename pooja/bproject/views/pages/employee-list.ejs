<%- include("../includes/header") %>


<div class="container mt-4">
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <table class="table table-dark table-striped">
                <tr>
                    <td>Sr.No.</td>
                    <td>Name</td>
                    <td>Salary</td>
                    <td>Address</td>
                </tr>
                <% result.forEach((x,n)=>{ %>
            <tr>
                <td><%= n+1  %></td>
                <td><%= x.name  %></td>
                <td><%= x.salary  %></td>
                <td><%= x.address  %></td>
                <td><a href="/employee/delete/<%= x._id %>"class="btn btn-danger">Delete</a></td>
                <td><a href="/employee/update/<%= x._id %>"class="btn btn-info">Edit</a></td>
            </tr>
               <% })  %>
            </table>
        </div>
    </div>
</div>
</div>
</body>
</html>