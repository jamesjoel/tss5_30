<%- include('header') %>        
        <div class="container mt-4">
            <div class="row">
                <div class="col-md-8 offset-md-2">
                    <table class="table table-dark">
                        <tr>
                            <th>S.No.</th>
                            <th>Name</th>
                            <th>Age</th>
                            <th>City</th>
                            <th>Fee</th>
                        </tr>

                        <% student.forEach((x, i)=>{ %>
                            <tr>
                                <td><%= i+1 %></td>
                                <td><%= x.name %></td>
                                <td><%= x.age %></td>
                                <td><%= x.city %></td>
                                <td><%= x.fee %></td>
                            </tr>
                        <% }) %>

                    </table>
                </div>
            </div>
        </div>
        
    </body>
</html>